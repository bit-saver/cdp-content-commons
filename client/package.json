{
  "name": "content-commons",
  "version": "1.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "aws-amplify": "^1.0.6",
    "aws-amplify-react": "^1.0.6",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bodybuilder": "^2.2.10",
    "dotenv": "^6.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "file-saver": "^1.3.8",
    "html-react-parser": "^0.4.6",
    "jszip": "^3.1.5",
    "jszip-utils": "0.0.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.5.0",
    "react-markdown": "^3.3.2",
    "react-redux": "^5.0.7",
    "react-redux-loading-bar": "^4.0.5",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "semantic-ui-react": "^0.79.0",
    "semantic-ui-sass": "^2.2.11",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "node-plop": "^0.16.0",
    "node-sass-chokidar": "1.3.0",
    "onchange": "^4.1.0",
    "plop": "^2.1.0",
    "postcss-cli": "^6.0.0",
    "prettier-eslint": "^8.8.1",
    "react-scripts": "1.1.4",
    "redux-devtools-extension": "^2.13.2",
    "rx": "^4.1.0",
    "version": "^0.1.2"
  },
  "scripts": {
    "precommit": "lint-staged",
    "post-css": "postcss ./src/**/*.css -u autoprefixer --base src --dir src/",
    "chok-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build-css": "npm run chok-css && npm run post-css",
    "watch-css": "npm run build-css && onchange 'src/**/*.scss' -- npm run chok-css && npm run post-css",
    "watch-css-old": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ && npm run post-css --watch --recursive",
    "start-js": "PORT=80 && react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "generate": "plop --plopfile generators/index.js"
  },
  "lint-staged": {
    "src/**/*.{js,json}": "eslint"
  },
  "browserslist": [
    "> 0.3%",
    "last 7 versions",
    "Android >= 4",
    "Firefox >= 20",
    "iOS >= 8"
  ]
}
